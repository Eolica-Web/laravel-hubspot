{
    "name": "eolica/laravel-hubspot",
    "description": "Laravel wrapper for the Hubspot PHP client package",
    "keywords": [
        "eolica",
        "laravel-hubspot",
        "laravel",
        "hubspot",
        "hubspot php",
        "hubspot api",
        "hubspot laravel"
    ],
    "homepage": "https://github.com/Eolica-Web/laravel-hubspot",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Llobell",
            "email": "dllobellmoya@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "graham-campbell/manager": "^5.0",
        "hubspot/hubspot-php": "^5.0",
        "illuminate/contracts": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Eolica\\LaravelHubspot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eolica\\LaravelHubspot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eolica\\LaravelHubspot\\HubspotServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
